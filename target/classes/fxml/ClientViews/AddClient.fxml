                </ImageView>
            </graphic>
        </JFXButton>
        <JFXButton fx:id="Cancel" layoutX="332.0" layoutY="446.0" onAction="#cancel_click" prefHeight="26.0" prefWidth="218.0" styleClass="SecBtn" stylesheets="@../../css/style.css" text="Cancel">
