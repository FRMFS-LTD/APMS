                        <Image url="@../../assets/ClientIco/icons8_name_64px_1.png" />
                    </image>
                    <HBox.margin>
                        <Insets right="3.0" />
                    </HBox.margin>
                </ImageView>
                <JFXTextField fx:id="firstname" onKeyPressed="#firstname_TextChanged" prefHeight="18.0" prefWidth="218.0" promptText="First Name" styleClass="textField" />
