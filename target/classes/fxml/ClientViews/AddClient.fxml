                    </image>
                    <HBox.margin>
                        <Insets right="3.0" />
                    </HBox.margin>
                </ImageView>
                <JFXTextField fx:id="cinclient" onKeyPressed="#cinclient_TextChanged" prefHeight="18.0" prefWidth="218.0" promptText="CIN" />
            </children>
        </HBox>
        <Label fx:id="cinError" layoutX="168.0" layoutY="342.0" prefHeight="18.0" prefWidth="218.0" />
        <Label fx:id="GlobalError" layoutX="52.0" layoutY="86.0" prefHeight="26.0" prefWidth="527.0" />
